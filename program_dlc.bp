Sub line_forward
  err = Math.Abs(Motor.GetCount(left_motor)) - Math.Abs(Motor.GetCount(right_motor))
  up = err * 1
  right_power = 60 + up
  left_power = 60 - up
  Motor.StartSync(motors, left_power * left_forward, right_power * right_forward)
EndSub

